    <declare-styleable name="AnyKeyboardViewTheme">
        <!-- Image for the key. This image needs to be a StateListDrawable, with the following
             possible states: normal, pressed, checkable, checkable+pressed, checkable+checked,
             checkable+checked+pressed. -->
        <attr format="reference" name="keyBackground"/>

        <!-- Size of the text for character keys. -->
        <attr format="reference|dimension" name="keyTextSize"/>

        <!-- Size of the text for custom keys with some text and no icon. -->
        <attr format="reference|dimension" name="labelTextSize"/>

        <!-- Color to use for the label in a key. -->
        <attr format="reference|color" name="keyTextColor"/>

        <!-- size of different keys height -->
        <attr format="reference|dimension" name="keyNormalHeight"/>
        <attr format="reference|dimension" name="keySmallHeight"/>
        <attr format="reference|dimension" name="keyLargeHeight"/>

        <attr format="reference|dimension" name="keyHorizontalGap"/>
        <attr format="reference|dimension" name="keyVerticalGap"/>

        <!-- Vertical offset of the key press feedback from the key. -->
        <attr format="reference|dimension" name="keyPreviewOffset"/>
        <attr format="reference" name="keyPreviewBackground"/>
        <attr format="reference|dimension" name="keyPreviewTextSize"/>
        <attr format="reference|color" name="keyPreviewTextColor"/>
        <attr format="reference|dimension" name="keyPreviewLabelTextSize"/>

        <!-- Hysteresis distance for key debouncing -->
        <attr format="reference|dimension" name="keyHysteresisDistance"/>

        <!-- Amount to offset the touch Y coordinate by, for bias correction. -->
        <attr format="reference|dimension" name="verticalCorrection"/>

        <attr format="reference|color" name="shadowColor"/>
        <attr format="reference|dimension" name="shadowRadius"/>
        <attr format="reference|dimension" name="shadowOffsetX"/>
        <attr format="reference|dimension" name="shadowOffsetY"/>

        <attr format="reference|float" name="backgroundDimAmount"/>

        <attr name="keyTextStyle">
            <flag name="normal" value="0"/>
            <flag name="bold" value="1"/>
            <flag name="italic" value="2"/>
        </attr>

        <attr name="keyTextCaseStyle">
            <flag name="auto" value="0"/>
            <flag name="lowercase" value="1"/>
            <flag name="uppercase" value="2"/>
        </attr>

        <attr name="symbolColorScheme">
            <flag name="white" value="0"/>
            <flag name="black" value="1"/>
        </attr>

        <attr name="android:background"/>
        <attr name="android:paddingLeft"/>
        <attr name="android:paddingTop"/>
        <attr name="android:paddingRight"/>
        <attr name="android:paddingBottom"/>

        <attr format="reference|color" name="suggestionNormalTextColor"/>
        <attr format="reference|color" name="suggestionRecommendedTextColor"/>
        <attr format="reference|color" name="suggestionOthersTextColor"/>
        <attr format="reference|dimension" name="suggestionTextSize"/>
        <attr format="reference" name="suggestionBackgroundImage"/>
        <attr format="reference" name="suggestionDividerImage"/>
        <attr format="reference" name="suggestionCloseImage"/>
        <attr format="reference|dimension" name="suggestionStripHeight"/>

        <!-- Size of the text for hinting about long press output. -->
        <attr format="reference|dimension" name="hintTextSize"/>

        <!-- Color to use for hint text. -->
        <attr format="reference|color" name="hintTextColor"/>

        <!-- Size of the text for the prefered word. -->
        <attr format="reference|dimension" name="suggestionPreferedWordTextSize"/>
        <!-- vertical padding. -->
        <attr format="reference|dimension" name="suggestionWordXGap"/>

        <!-- location of hint labels -->
        <attr name="hintLabelAlign">
            <flag name="left" value="3"/>
            <flag name="center" value="17"/>
            <flag name="right" value="5"/>
        </attr>
        <attr name="hintLabelVAlign">
            <flag name="top" value="48"/>
            <flag name="bottom" value="80"/>
            <!-- currently not supported -->
            <flag name="center" value="17"/>
        </attr>

        <!-- hint label to use when hintLabel unspecified and more than 3 characters in -->
        <!-- popupCharacters or when longPressCode instead of popupCharacters -->
        <!-- (if none specified, app default used) -->
        <attr format="reference|string" name="hintOverflowLabel"/>

        <!-- FOR FUTURE EXPANSION - NOT YET SUPPORTED -->

        <!-- hint icon to use when no hintLabel given and more than 3 characters in popupCharacters -->
        <!-- or when longPressCode instead of popupCharacters (if not specified, app default used) -->
        <!-- if both icon and label specified, icon wins -->
        <attr format="reference" name="hintOverflowIcon"/>
        <!-- allow the key to have another hint to show that there are more characters in -->
        <!-- popupCharacters than shown by the hint label (if not specified, none used) -->
        <attr format="reference|string" name="extraHintLabel"/>
        <attr format="reference" name="extraHintIcon"/>
        <attr name="extraHintLabelAlign">
            <flag name="left" value="3"/>
            <flag name="center" value="17"/>
            <flag name="right" value="5"/>
        </attr>
        <attr name="extraHintLabelVAlign">
            <flag name="top" value="48"/>
            <flag name="bottom" value="80"/>
            <!-- less likely to support... -->
            <flag name="center" value="17"/>
        </attr>
        <!-- allow main key label/icon to be in different positions -->
        <!-- behavior before implemented: keyLabelIconAlign="center" keyLabelIconVAlign="auto" -->
        <attr name="keyLabelIconAlign">
            <!-- against left or right edge of key -->
            <flag name="left" value="3"/>
            <flag name="right" value="5"/>
            <!-- horizontally centered at center of key (adjusted only for key padding) -->
            <flag name="center" value="17"/>
            <!-- centered, but also adjust position to account for hint label location -->
            <flag name="auto" value="7"/>
        </attr>

        <attr name="keyLabelIconVAlign">
            <!-- against top or bottom edge of key -->
            <flag name="top" value="48"/>
            <flag name="bottom" value="80"/>
            <!-- vertically centered at center of key (adjusted only for key padding) -->
            <flag name="center" value="17"/>
            <!-- centered, but also adjust position to account for hint label location -->
            <flag name="auto" value="112"/>
        </attr>
        <!-- expected behavior if showHintOnPreview="false" and showExtraHintOnPreview="true" -->
        <!-- is that extra hint is shown if any long press or popupCharacters for key -->

        <!-- keyboard wallpaper - this is the image BEHIND the keyboard, it is visible when switching layout with animation -->
        <attr format="reference" name="keyboardWallpaper"/>

        <attr format="reference|dimension" name="keyboardNameTextSize"/>
        <attr format="reference|color" name="keyboardNameTextColor"/>

        <attr format="reference|dimension" name="previewGestureTextSize"/>
        <attr format="reference|color" name="previewGestureTextColor"/>
        <!-- show hint and/or extra hint on preview -->
        <attr format="reference|boolean" name="showHintOnPreview"/>
        <attr format="reference|boolean" name="showExtraHintOnPreview"/>
        <attr name="previewAnimationType">
            <flag name="none" value="0"/>
            <!--default value-->
            <flag name="extend_key" value="1"/>
            <flag name="appear" value="2"/>
        </attr>

        <attr format="reference|color" name="swipeTypingColor"/>
        <attr format="reference|dimension" name="swipeTypingStrokeWidth"/>
        <attr format="reference|dimension" name="swipeTypingStrokeMinWidth"/>
        <attr format="reference|dimension" name="swipeTypingStrokeWidthStep"/>
        <attr format="reference|float" name="swipeTypingStrokeMinAlpha"/>
        <attr format="reference|integer" name="swipeTypingStrokeAlphaStep"/>
        <attr format="reference|color" name="swipeTypingShadowColor"/>
        <attr format="reference|dimension" name="swipeTypingShadowRadius"/>
        <attr format="reference|dimension" name="swipeTypingShadowOffsetX"/>
        <attr format="reference|dimension" name="swipeTypingShadowOffsetY"/>
    </declare-styleable>
    <declare-styleable name="KeyboardLayout">
        <!-- Default width of a key, in pixels or percentage of display width. -->
        <attr name="android:keyWidth"/>
        <!-- Default height of a key, in pixels or percentage of display width. -->
        <attr name="android:keyHeight"/>
        <!-- Default horizontal gap between keys. -->
        <attr name="android:horizontalGap"/>
        <!-- Default vertical gap between rows of keys. -->
        <attr name="android:verticalGap"/>
        <!-- Whether should show preview. -->
        <attr name="showPreview"/>
    </declare-styleable>
    <declare-styleable name="KeyboardLayout_Key">
        <!-- The unicode value or comma-separated values that this key outputs. -->
        <attr name="android:codes"/>

        <!-- The XML keyboard layout of any popup keyboard. -->
        <attr name="android:popupKeyboard"/>
        <!-- The characters to display in the popup keyboard. -->
        <attr name="android:popupCharacters"/>
        <!-- Key edge flags. -->
        <attr name="android:keyEdgeFlags">
        </attr>
        <!-- Whether this is a modifier key such as Alt or Shift. -->
        <attr name="android:isModifier"/>
        <!-- Whether this is a toggle key. -->
        <attr name="android:isSticky"/>
        <!-- Whether long-pressing on this key will make it repeat. -->
        <attr name="android:isRepeatable"/>
        <!-- The icon to show in the popup preview. -->
        <attr name="android:iconPreview"/>
        <!-- The string of characters to output when this key is pressed. -->
        <attr name="android:keyOutputText"/>
        <!-- The label to display on the key. -->
        <attr name="android:keyLabel"/>
        <!-- The icon to display on the key instead of the label. -->
        <attr name="android:keyIcon"/>
        <!-- Mode of the keyboard. If the mode doesn't match the
             requested keyboard mode, the key will be skipped. -->
        <attr name="android:keyboardMode"/>

        <attr format="reference|integer" name="longPressCode"/>

        <attr format="reference|boolean" name="isFunctional"/>

        <attr format="reference|integer|string" name="shiftedCodes"/>

        <attr format="reference|string" name="shiftedKeyLabel"/>

        <!-- Whether should show preview. -->
        <attr name="showPreview"/>

        <attr name="keyDynamicEmblem">
            <flag name="none" value="0"/>
            <flag name="text" value="1"/>
            <flag name="icon" value="2"/>
        </attr>

        <!-- The characters to display as a hint for the popup keyboard -->

        <!-- The characters to display as a hint for the popup keyboard -->
        <attr format="reference|string" name="hintLabel"/>

        <attr name="showInLayout">
            <!--default value-->
            <flag name="always" value="0"/>
            <flag name="if_applicable" value="1"/>
            <flag name="never" value="2"/>
        </attr>

        <!-- coma separated tags for this key. Might be used for completions -->
        <attr format="reference|string" name="tags"/>

        <attr format="reference|string" name="extra_key_data"/>
    </declare-styleable>
    <declare-styleable name="KeyboardLayout_Row">
        <!-- Row edge flags. -->
        <attr name="android:rowEdgeFlags">
        </attr>
        <!-- Mode of the keyboard. If the mode doesn't match the
             requested keyboard mode, the row will be skipped. -->
        <attr name="android:keyboardMode"/>
    </declare-styleable>
    <declare-styleable name="keyActionType">
        <attr format="boolean" name="action_done"/>
        <attr format="boolean" name="action_search"/>
        <attr format="boolean" name="action_go"/>
        <attr format="boolean" name="action_send"/>
        <attr format="boolean" name="action_previous"/>
        <attr format="boolean" name="action_next"/>
    </declare-styleable>
    <declare-styleable name="keyType">
        <attr format="boolean" name="key_type_function"/>
        <attr format="boolean" name="key_type_action"/>
        <attr format="boolean" name="key_type_feedback"/>
    </declare-styleable>
</resources>
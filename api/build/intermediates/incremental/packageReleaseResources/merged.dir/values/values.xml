        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyInputClear"/>
        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyInputSelectAll"/>
        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyInputClipboardCopy"/>
        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyInputClipboardCut"/>
        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyInputClipboardPaste"/>
        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyInputMoveEnd"/>
        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyInputMoveHome"/>
        <!-- should support normal, key_type_feedback -->
        <attr format="reference" name="iconKeyCondenseNormal"/>
        <!-- should support normal, key_type_feedback -->
